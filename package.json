{
  "name": "projeto-selenium",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build -aot -prod -sm",
    "test": "ng test --colors --hostname=$(hostname -i)",
    "e2e": "echo confira se seu ip está correto, senão corrija no /etc/hosts: $(hostname -i)  && ng e2e --host $(hostname -i)",
    "test:ci": "ng test --config=karma.conf.ci.js --single-run --hostname=$(hostname -i) --code-coverage",
    "test:ci:travis": "ng test --config=karma.conf.ci.js --single-run --hostname=172.17.0.1 --code-coverage",
    "e2e:ci": "ng e2e --host $(hostname -i) --config protractor.conf.ci.js",
    "e2e:ci:travis": "ng e2e --host 172.17.0.1 --config protractor.conf.ci.js",
    "lint": "ng lint",
    "compodoc": "compodoc -p src/tsconfig.app.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.3.0",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.1.1",
    "compodoc": "^0.0.41",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-verbose-reporter": "^0.0.6",
    "karma-webdriver-launcher": "^1.0.5",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
